package juneb_testN05;

public class Testno_5
{
	public static void main(String[] args)
	{


		String str1 = "something";
		String str2 = str1.intern();
		String str3 = new String("something");
		System.out.println(str3.hashCode());
		String str4 = str1.intern();
		System.out.println(str2 == str4);
		System.out.println(str1.hashCode() + "\n"+str3.hashCode());
		System.out.println("**************************************");
		
		
		String s1 = "something";   
		final String s2 = "anything";   
		String s3 = "something".concat(s2);   
		String s4 = "something".concat("anything");   
		System.out.println(s3 == s4);  
		String s5 = "something" + (s2);  
		String s6 = "something" + "anything";   
		System.out.println(s5 == s6);  
		System.out.println("**************************************");
		
		String str = "java";  
		int i, k = 4;  
		while (k <= str.length()) 
		{   
			i = 1;   
			k = k - i;    
			System.out.println("No Pain No Gain"); 
		}
	}

}
