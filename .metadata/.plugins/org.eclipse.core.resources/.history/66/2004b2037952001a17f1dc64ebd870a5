package com.previousquetion;

public class SpiralMatrix {

	public static void main(String[] args) {
		
		int index=0;
		
		int left=0;
		int top=0;
		int right=5-1;
		int bottom=5-1;
		
		int arr[][] =new int[4][4];
		
		while(true) {
			
			if(left>right) {
				break;
			}
			for(int i=left;i<right; i++) {
				arr[top][i]=index++;
			}
		}

	}

}
