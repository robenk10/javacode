package arrayAssingment;

public class Push0atbegin
{

	public static void main(String[] args) 
	{
		int[] arr= {1,0,5,0,6,7};
		int temp;
		int count=0;
		for(int i=arr.length-1;i>=0;i--)
		{
			if(arr[i]!=0)
			{
				temp=arr[i];
				arr[i]=arr[count+1];
				arr[count+1]=temp;
				count++;
			}
			
		}
		
		for(int i=0;i<arr.length;i++)
		{
			System.out.print(arr[i]);
		}


	}

}
