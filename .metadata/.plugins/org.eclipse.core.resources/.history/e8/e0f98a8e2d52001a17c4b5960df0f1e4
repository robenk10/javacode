package com.AssignmentOfCW;

import java.util.*;

public class TargetSum {

	public static void main(String[] args) {
		
		int [] arr= {10,0,-1,20,25,30};
		
		int target=59;
		ArrayList<Integer> list=new ArrayList();
	
		for(int i:arr) {
			list.add(i);
		}
		int sum=0;
		for(int i=0; i<arr.length; i++) {
			sum=sum+arr[i];
		}
        
		System.out.println(sum);
	}

}
