package arrayAssingment;

public class Push0atbegin
{

	public static void main(String[] args) 
	{
		int[] arr= {1,0,5,0,6,7};
		int temp;
		int count=1;;
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]!=0)
			{
				temp=arr[i];
				arr[i]=arr[count];
				arr[count]=temp;
				count++;
			}
			
		}
		
		for(int i=0;i<arr.length;i++)
		{
			System.out.print(" "+arr[i]);
		}


	}

}
